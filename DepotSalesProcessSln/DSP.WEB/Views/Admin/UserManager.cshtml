@model DSP.Core.DTO.AppUser.AppUsersDTO
@{
    ViewData["Title"] = "UserManager";
}

<h1>UserManager</h1>
<div class="row">
    <div class="col-md-12">
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AppUser.Type"></label>
                <input asp-for="AppUser.Type" class="form-control" />
                <span asp-validation-for="AppUser.Type" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.UserName"></label>
                <input asp-for="AppUser.UserName" class="form-control" />
                <span asp-validation-for="AppUser.UserName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.Password"></label>
                <input asp-for="AppUser.Password" class="form-control" />
                <span asp-validation-for="AppUser.Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.SAPBranch"></label>
                <input asp-for="AppUser.SAPBranch" class="form-control" />
                <span asp-validation-for="AppUser.SAPBranch" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.Warehouse"></label>
                <input asp-for="AppUser.Warehouse" class="form-control" />
                <span asp-validation-for="AppUser.Warehouse" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.VendorCode"></label>
                <input asp-for="AppUser.VendorCode" class="form-control" />
                <span asp-validation-for="AppUser.VendorCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.CustomerCode"></label>
                <input asp-for="AppUser.CustomerCode" class="form-control" />
                <span asp-validation-for="AppUser.CustomerCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.ContactNo"></label>
                <input asp-for="AppUser.ContactNo" class="form-control" />
                <span asp-validation-for="AppUser.ContactNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.EmailAddress"></label>
                <input asp-for="AppUser.EmailAddress" class="form-control" />
                <span asp-validation-for="AppUser.EmailAddress" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.LicenseAssign"></label>
                <input asp-for="AppUser.LicenseAssign" class="form-control" />
                <span asp-validation-for="AppUser.LicenseAssign" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.Active"></label>
                <input asp-for="AppUser.Active" class="form-control" />
                <span asp-validation-for="AppUser.Active" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AppUser.Approver"></label>
                <input asp-for="AppUser.Approver" class="form-control" />
                <span asp-validation-for="AppUser.Approver" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-primary">Create</button>
        </form>
    </div>
</div>